Include ~/.orbstack/ssh/config

Host *
  AddKeysToAgent yes

# Github
Host github.com *.github.com
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519_github
  IdentitiesOnly yes

# Azure DevOps - MCP-TenneT (default)
Host tennet-mcp
  HostName ssh.dev.azure.com
  User git
  IdentityFile ~/.ssh/rsa_4096_tennet_mcp
  IdentitiesOnly yes

# Azure DevOps - IDC-TenneT (works through url rewrite in git)
# git@ssh.dev.azure.com:v3/tennetidc/* --> git@tennet-idc:v3/tennetidc/*
Host tennet-idc
  HostName ssh.dev.azure.com
  User git
  IdentityFile ~/.ssh/rsa_4096_tennet_idc
  IdentitiesOnly yes

# Meterkast server
Host meterkast
  HostName 192.168.50.103
  User dennis
  IdentityFile ~/.ssh/id_ed25519_meterkast
  IdentitiesOnly yes

# Meterkast server
Host synology
  HostName 192.168.50.102
  User dennis
  IdentityFile ~/.ssh/id_ed25519_synology
  IdentitiesOnly yes

# VPS
Host etheron
  HostName 5.181.134.208
  User root
  IdentityFile ~/.ssh/id_ed25519_etheron
  IdentitiesOnly yes
