Include ~/.orbstack/ssh/config

Host *
  HostkeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa
  AddKeysToAgent  yes

Host *.github.com
  AddKeysToAgent yes

Host azure-ca
  HostName ssh.dev.azure.com
  User git
  IdentityFile ~/.ssh/id_rsa_devops_ca_account
  IdentitiesOnly yes

Host azure-ka
  HostName ssh.dev.azure.com
  User git
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes

